{
  "modName": "MaintainedMagic",
  "displayName": "Maintained Magic NG",
  "minMcmVersion": 11,
  "cursorFillMode": "topToBottom",

  "pages": [
    {
      "pageDisplayName": "General",
      "cursorFillMode": "topToBottom",
      "content": [
        {
          "text": "General Settings",
          "type": "header"
        },
        {
          "id": "bAllowBoundWeapons:General",
          "text": "Allow Maintained Bound Weapons",
          "type": "toggle",
          "help": "Allows proper bound weapon spells to be maintained.\n\nBound weapons must be re-summoned after unsheathing, and maintenance will break if the weapon or its spell is removed from your hand. Script-based bound items are not supported.",
          "valueOptions": {
            "sourceType": "ModSettingBool"
          }
        },
        {
          "id": "bInstantDispel:General",
          "text": "Instant Dispel on Recast",
          "type": "toggle",
          "help": "When enabled, recasting a maintained spell immediately dispels its maintained effects instead of applying the spell with its normal duration.",
          "valueOptions": {
            "sourceType": "ModSettingBool"
          }
        },

        { "type": "empty" },

        {
          "text": "Upkeep Cost Scaling",
          "type": "header"
        },
        {
          "id": "iCostBaseDuration:Costs",
          "text": "Neutral Duration (seconds)",
          "type": "slider",
          "help": "Reference duration used for upkeep cost scaling.\n\nIf a maintained spellâ€™s duration equals this value, upkeep equals its base magicka cost.",
          "valueOptions": {
            "min": 0,
            "max": 300,
            "step": 15,
            "sourceType": "ModSettingInt"
          }
        },
        {
          "id": "fUpkeepDurationExponent:Costs",
          "text": "Upkeep Duration Exponent",
          "type": "slider",
          "help": "Controls how aggressively upkeep cost scales with duration.\n\nLOWER values increase upkeep cost.\nHIGHER values reduce upkeep cost.\n\nDoes not retroactively affect already maintained spells.",
          "valueOptions": {
            "min": 0.1,
            "max": 0.8,
            "step": 0.05,
            "formatString": "{2}",
            "sourceType": "ModSettingFloat"
          }
        },

        { "type": "empty" },

        {
          "text": "Experience Settings",
          "type": "header"
        },
        {
          "id": "fMaintainedExpMultiplier:Experience",
          "text": "Maintained Spell Experience Multiplier",
          "type": "slider",
          "help": "Passive experience gained from maintained spells.\n\nSet to 0 to disable.",
          "valueOptions": {
            "min": 0.0,
            "max": 5.0,
            "step": 0.1,
            "formatString": "{1}",
            "sourceType": "ModSettingFloat"
          }
        }
      ]
    },

